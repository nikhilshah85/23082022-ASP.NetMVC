

<h1> Top Selling Products </h1>

<table class="table">
    @foreach(var item in @ViewBag.products)
    {
        <tr>
            <td> @item.pId </td>    
            <td> @item.pName </td>    
            <td> @item.pCatergory </td>    
            <td> @item.pIsInStock </td>    
            <td> @item.pPrice </td>   
            <td><button class="btn btn-success"> Add To Cart </button></td>
        </tr>
    }
</table>